    "from sage.all import sage_eval\n",
    "\n",
    "def evaluate_with_sage(expression):\n",
    "    try:\n",
    "        result = sage_eval(expression)\n",
    "        return result\n",
    "    except Exception as e:\n",
    "        print(f\"Error evaluating expression: {e}\")\n",
    "\n",
    "expression = \"sin(pi/4)\"\n",
    "result = sage_eval(expression)\n",
    "print(result)"
   ]
